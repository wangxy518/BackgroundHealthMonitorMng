<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onesun.hbm.base.background.ThirdpartyUser" table="ThirdpartyManage">
		<id name="id" type="string">
			<column name="id" />
			<generator class="uuid"></generator>
		</id>
		<property name="name" type="string">
		  <column name="name" length="30"></column>
		</property>
		<property name="username" type="string">
		 <column name="username" length="30"></column>
		</property>
		<property name="password" type="string">
		  <column name="password" length="50"></column>
		</property>
		<property name="token" type="string">
		  <column name="token" length="50"></column>
		</property>
		<property name="healthExamination" type="string">
		  <column name="healthExamination" length="255"></column>
		</property>
		<property name="healthIndicator" type="string">
		  <column name="healthIndicator" length="255"></column>
		</property>
		<property name="interfaceName" type="string">
		  <column name="interfaceName" length="100"></column>
		</property>
		<property name="status" type="string">
		  <column name="status" length="10"></column>
		</property>
	</class>
</hibernate-mapping>