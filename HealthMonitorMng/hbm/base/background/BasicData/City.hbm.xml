<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="onesun.hbm.base.background.BasicData.City" table="Cities">
		<id name="id" type="string">
			<column name="id" />
			<generator class="assigned"></generator>
		</id>
		<property name="cityid" type="string">
		  <column name="cityid" length="30"></column>
		</property>
		<property name="city" type="string">
		  <column name="city" length="20"></column>
		</property>
		<property name="provinceid" type="string">
		  <column name="provinceid" length="30"></column>
		</property>
	</class>
</hibernate-mapping>